{
  "name": "table-search-fullstack",
  "version": "1.2.0",
  "description": "Fullstack table search example with FTS, fuzzy client search, edit stories with auth, inline previews and photos",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:win": "powershell -NoProfile -ExecutionPolicy Bypass -Command \"$env:ADMIN_PASSWORD='yourpass'; node server.js\"",
    "start:win-cmd": "set ADMIN_PASSWORD=yourpass && node server.js",
    "dev": "nodemon server.js",
    "init-db": "node init_db.js"
  },
  "dependencies": {
    "better-sqlite3": "^8.3.0",
    "dotenv": "^16.0.0",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.22"
  }
}